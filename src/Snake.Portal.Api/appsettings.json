{
  "Logging": {
    "PathFormat": "Logs/log-{Date}.txt",
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information"
    }
  },
  "StaticFiles": {
    "Headers": {
      "Cache-Control": "no-cache, no-store",
      "Pragma": "no-cache",
      "Expires": "-1"
    }
  },
  "Auth0": {
    "Domain": "jvcordina.eu.auth0.com",
    "ApiIdentifier": "http://localhost:5000/api"
  }
}
